{
  "name": "react-split-pane",
  "description": "React split-pane component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "source": "src/index.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "version": "0.1.92",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomkp/react-split-pane"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomkp/react-split-pane"
  },
  "homepage": "https://github.com/tomkp/react-split-pane",
  "author": "tomkp <tom@tomkp.com>",
  "keywords": [
    "react",
    "react-component",
    "split-pane",
    "react-split-pane",
    "es6"
  ],
  "scripts": {
    "start": "parcel website/index.html",
    "prebuild": "yarn run clean",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "clean": "rimraf dist",
    "test": "BABEL_ENV=test mochify -R spec --transform babelify",
    "test:watch": "BABEL_ENV=test mochify -R spec --watch --transform babelify",
    "test:coverage": "BABEL_ENV=test mochify --plugin [ mochify-istanbul --exclude '**/test/**' --report lcovonly ] --transform babelify && cat lcov.info | coveralls && rm lcov.info",
    "prettier": "prettier --write '{src,test}/**/*.js'",
    "lint": "eslint src test",
    "release": "standard-version",
    "build:storybook": "build-storybook -o build",
    "storybook": "start-storybook -p ${PORT:-3001} -h 0.0.0.0",
    "deploy": "gh-pages -d build",
    "prepublishOnly": "yarn run test && yarn run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react-lifecycles-compat": "^3.0.4",
    "react-style-proptype": "^3.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.10.5",
    "@storybook/addons": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/theming": "^6.5.14",
    "@types/react": "^18.0.25",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^9.1.0",
    "babelify": "^10.0.0",
    "chai": "^4.3.7",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-config-fbjs": "^4.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-relay": "^1.8.3",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "mochify": "^9.2.0",
    "mochify-istanbul": "^2.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.8.0",
    "pretty-quick": "^3.1.3",
    "react": ">=16",
    "react-dom": ">=16",
    "react-router-dom": "^6.4.4",
    "rimraf": "^3.0.2",
    "rollup": "^3.5.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  }
}